{
  "name": "weather-language",
  "displayName": "Weather Language",
  "description": "Adds support for the Weather programming language",
  "repository": {
    "type": "git",
    "url": "https://github.com/zanderlewis/weather"
  },
  "publisher": "zanderlewis",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.94.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "weather",
      "aliases": ["Weather", "weather", "wthr"],
      "extensions": [".wthr"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "weather",
      "scopeName": "source.weather",
      "path": "./syntaxes/weather.tmLanguage.json"
    }]
  }
}
